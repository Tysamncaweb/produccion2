<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
    	<record model="ir.ui.view" id="res_partner_callcenter_view_form">
            <field name="name">res.partner.callcenter.view.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="active_client" string="Solvente"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>