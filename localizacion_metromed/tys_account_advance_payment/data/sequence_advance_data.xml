<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Payment sequences -->
        <record id="sequence_register_payment_advance_supplier" model="ir.sequence">
            <field name="name">Payments advance supplier sequence</field>
            <field name="code">register.payment.advance.supplier</field>
            <field name="prefix">ADV.OUT/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_register_receivable_advance_customer" model="ir.sequence">
            <field name="name">Payments advance customer sequence</field>
            <field name="code">register.receivable.advance.customer</field>
            <field name="prefix">ADV.IN/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>